{
  "name": "politsin/srvr",
  "description": "Server Install.",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "composer/installers": "^2.1",
    "cweagans/composer-patches": "^1.6",
    "symfony/dotenv": "^6",
    "symfony/console": "^6.3",
    "symfony/yaml": "^6.3",
    "symfony/process": "^6.3"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Srvr\\": "src"
    }
  },
  "require-dev": {
    "symfony/var-dumper": "*"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true
    }
  }
}
