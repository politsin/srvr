{
  "workbench.startupEditor": "newUntitledFile",
  "workbench.iconTheme": "vscode-icons",
  "workbench.colorTheme": "One Dark Pro",
  "security.workspace.trust.enabled": false,
  "vsicons.dontShowNewVersionMessage": true,
  "problems.autoReveal": false,
  "explorer.confirmDelete": true,
  "explorer.confirmDragAndDrop": true,
  "update.showReleaseNotes": false,
  "window.zoomLevel": 0,
  "window.commandCenter": false,
  "extensions.ignoreRecommendations": false,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.formatOnPaste": true,
  "editor.minimap.showSlider": "always",
  "editor.minimap.side": "left",
  "editor.minimap.renderCharacters": false,
  "editor.minimap.maxColumn": 80,
  "editor.renderWhitespace": "none",
  "editor.wordSeparators": "`~!@#%^&*()-=+[{]}\\|;:'\",.<>/?",
  "editor.semanticTokenColorCustomizations": {},
  "editor.guides.bracketPairs": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "git.autorefresh": false,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.associations": {
    "*.module": "php",
    "*.theme": "php",
    "*.inc": "php"
  },
  "php.validate.enable": false,
  "php.suggest.basic": false,
  "phpsab.composerJsonPath": "/var/www/html/composer.json",
  "phpsab.executablePathCBF": "/var/lib/composer/vendor/bin/phpcbf",
  "phpsab.executablePathCS": "/var/lib/composer/vendor/bin/phpcs",
  "phpsab.snifferMode": "onType",
  "[php]": {
    "editor.defaultFormatter": "valeryanm.vscode-phpsab"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "codeium.enableConfig": {
    "*": true,
    "markdown": true
  },
  "codeium.enableCodeLens": false,
  "intelephense.licenceKey": "00EHGVL5TQGS1S0",
  "intelephense.environment.documentRoot": "/var/www/html",
  "intelephense.stubs": [
    "apache",
    "bcmath",
    "bz2",
    "calendar",
    "com_dotnet",
    "Core",
    "ctype",
    "curl",
    "date",
    "dba",
    "dom",
    "enchant",
    "exif",
    "FFI",
    "fileinfo",
    "filter",
    "fpm",
    "ftp",
    "gd",
    "gettext",
    "gmp",
    "hash",
    "iconv",
    "imap",
    "intl",
    "json",
    "ldap",
    "libxml",
    "mbstring",
    "meta",
    "mysqli",
    "oci8",
    "odbc",
    "openssl",
    "pcntl",
    "pcre",
    "PDO",
    "pdo_ibm",
    "pdo_mysql",
    "pdo_pgsql",
    "pdo_sqlite",
    "pgsql",
    "Phar",
    "posix",
    "pspell",
    "random",
    "readline",
    "Reflection",
    "session",
    "shmop",
    "SimpleXML",
    "snmp",
    "soap",
    "sockets",
    "sodium",
    "SPL",
    "sqlite3",
    "standard",
    "superglobals",
    "sysvmsg",
    "sysvsem",
    "sysvshm",
    "tidy",
    "tokenizer",
    "xml",
    "xmlreader",
    "xmlrpc",
    "xmlwriter",
    "xsl",
    "Zend OPcache",
    "zip",
    "zlib",
    "redis"
  ],
  "intelephense.phpdoc.classTemplate": {
    "summary": "$1",
    "tags": ["@package ${1:$SYMBOL_NAMESPACE}"]
  },
  "intelephense.phpdoc.propertyTemplate": {
    "summary": "$1",
    "tags": ["@var ${1:$SYMBOL_TYPE}"]
  },
  "editor.rulers": []
}
